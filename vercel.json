{
  "version": 2,
  "builds": [
    {
      "src": "./index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ],
  "functions": {
    "myFunction": {
      "runtime": "@vercel/node",
      "memory": 1024
    }
  },
  "install": {
    "include": [
      "package.json"
    ]
  }
}